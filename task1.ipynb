   "source": [
    "import numpy as np\n",
    "\n",
    "X = np.random.normal(loc = 1, scale = 10, size = (1000,50))\n",
    "\n",
    "m = np.mean(X, axis=0)\n",
    "std = np.std(X, axis=0)\n",
    "X_norm = ((X - m)  / std)\n",
    "\n",
    "print(X_norm)"
   ]
